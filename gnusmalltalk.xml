	<RegExpr attribute="Operator" context="#stay" String="[,]{1}"  /> 
	<RegExpr attribute="Comment2" context="Comment2" String="[&quot;][_]" beginRegion="Comment2"/>
	<RegExpr attribute="Comment" context="Comment" String="[&quot;]" beginRegion="Comment"/>
	<RegExpr attribute="Symbol" context="#stay" String="\#[a-zA-Z][\w]*"/>
	<RegExpr attribute="Char" context="#stay" String="\$."/>
	<DetectChar attribute="String" context="String" char="'" beginRegion="String"/>
	<DetectChar attribute="Brackets1" context="Level1" char="[" beginRegion="Brackets1"/>
	<DetectChar attribute="BracketsA" context="LevelA" char="(" beginRegion="BracketsA"/>
	<RegExpr attribute="Allocation" context="#stay" String="\w+[\s|\t]*:=" />
        <RegExpr attribute="Integer" context="#stay" String="[0-9]+" />
	<RegExpr attribute="Class" context="#stay" String="\b([A-Z]+\w+)"/>
	<RegExpr attribute="Variable" context="Variable" String="[\|]" beginRegion="Variable"/> 
	<RegExpr attribute="Variable2" context="#stay"  String="(([\:][\s|\t]*[a-zA-Z]+[\s|\t]*)+[\s|\t]*)+[\|]{1}"/>  
	<AnyChar attribute="Operator" context="#stay" String="+-;=*~&lt;&gt;^.@|&amp;"/>  
	<keyword attribute="Keyword" context="#stay" String="keywords" />
	<!-- "no lookbehind-rule" 
	  <RegExpr attribute="VariableOrObject" context="#stay" String="(?:[^a-zA-Z0-9_\s\t])[\s|\t]*[a-z][\w]*"  /> 
	  <RegExpr attribute="VariableOrObject" context="#stay" fallthrough="true" String="([\:][\s|\t]*[a-zA-Z0-9_]+)+[\s|\t]*"/>  
	-->
	<keyword attribute="Keyword" context="#stay" String="keywords" />
    </context>

